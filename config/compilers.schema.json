{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Compilers",
    "description": "A list of compilers that are supported",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "COMPILER_NAME": {
                "type": "string"
            },
            "COMPILER_VERSION": {
                "type": "string"
            },
            "COMPILER_PKG_NAME": {
                "type": "string"
            },
            "COMPILER_PKG_VERSION": {
                "type": "string"
            }
        },
        "required": [ "COMPILER_NAME", "COMPILER_VERSION", "COMPILER_PKG_NAME", "COMPILER_PKG_VERSION" ],
        "additionalProperties": false
    }
}